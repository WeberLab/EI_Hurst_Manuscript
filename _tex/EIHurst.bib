  keywords = {Functional magnetic resonance spectroscopy,glucose,Glucose,glutamate,Glutamic Acid,Hemodynamics,Humans,Magnetic Resonance Spectroscopy,negative blood oxygenation level-dependent response,Oxygen,Oxygen Consumption,positive blood oxygenation level-dependent response,Visual Cortex},
