  keywords = {1H-MRS,relaxation times,spectroscopic imaging,tissue water,voxel},
  file = {/home/weberam2/Zotero/storage/CBR87SG9/Gasparovic_et_al_2006_Use_of_tissue_water_as_a_concentration_reference_for_proton_spectroscopic.pdf}
