@article{ipCombinedFMRIMRSAcquires2017,
  title = {Combined {{fMRI-MRS}} Acquires Simultaneous Glutamate and {{BOLD-fMRI}} Signals in the Human Brain},
  author = {Ip, I. Betina and Berrington, Adam and Hess, Aaron T. and Parker, Andrew J. and Emir, Uzay E. and Bridge, Holly},
