@article{mullinsNovelTechniqueStudy2005,
  title = {A Novel Technique to Study the Brain's Response to Pain: Proton Magnetic Resonance Spectroscopy},
  shorttitle = {A Novel Technique to Study the Brain's Response to Pain},
  author = {Mullins, Paul G. and Rowland, Laura M. and Jung, Rex E. and Sibbitt, Wilmer L.},
  year = {2005},
  month = jun,
  journal = {NeuroImage},
  volume = {26},
  number = {2},
  pages = {642--646},
  issn = {1053-8119},
  doi = {10.1016/j.neuroimage.2005.02.001},
  abstract = {Glutamate, a major excitatory neurotransmitter, has been implicated as an important mediator in the neurotransmission, potentiation, and negative affect associated with pain. We present results showing that a painful stimulus elicits a dynamic increase in glutamate (9.3\% from baseline) concentrations in the anterior cingulate cortex, detectable using proton Magnetic Resonance Spectroscopy ((1)H-MRS). Increases in glutamine levels were also seen, which correlate strongly with the subjective level of pain experienced by participants (r(2) = 0.58, P {$<$} 0.01). These novel findings are the first time a dynamic change in glutamate and glutamine levels from baseline in response to an external stimuli has been measured in a single proton MRS scanning session. As such, this report demonstrates the efficacy of (1)H-MRS as a non-invasive tool for the study of neural responses to pain in vivo. The paradigm used in this study demonstrates that dynamic glutamate/glutamine changes due to stimulation are measurable by proton MRS, and could provide a means of testing novel pharmaceutical agents and other treatments for chronic pain.},
  langid = {english},
  pmid = {15907322},
  keywords = {Adult,Brain,Brain Chemistry,Cold Temperature,Female,Glutamic Acid,Glutamine,Humans,Image Interpretation Computer-Assisted,Magnetic Resonance Imaging,Male,Pain,Pain Measurement,Pressure,Protons,Synaptic Transmission}
}

@article{mullinsTheoryFunctionalMagnetic2018,
  title = {Towards a Theory of Functional Magnetic Resonance Spectroscopy ({{fMRS}}): {{A}} Meta-Analysis and Discussion of Using {{MRS}} to Measure Changes in Neurotransmitters in Real Time},
  shorttitle = {Towards a Theory of Functional Magnetic Resonance Spectroscopy ({{fMRS}})},
  author = {Mullins, Paul G.},
  year = {2018},
  month = feb,
  journal = {Scandinavian Journal of Psychology},
  volume = {59},
  number = {1},
  pages = {91--103},
  issn = {1467-9450},
  doi = {10.1111/sjop.12411},
  abstract = {Proton magnetic resonance spectroscopy is a powerful tool to investigate neurochemistry and physiology in~vivo. Recently researchers have started to use MRS to measure neurotransmitter changes related to neural activity, so called functional MRS (fMRS). Particular interest has been placed on measuring glutamate changes associated with neural function, but differences are reported in the size of changes seen. This review discusses fMRS, and includes meta-analyses of the relative size of glutamate changes seen in fMRS, and the impact experimental design and stimulus paradigm may have. On average glutamate was found to increase by 6.97\% ({\textpm}1.739\%) in response to neural activation. However, factors of experimental design may have a large impact on the size of these changes. For example an increase of 4.749\% ({\textpm}1.45\%) is seen in block studies compared to an increase of 13.429\% ({\textpm}3.59) in studies using event related paradigms. The stimulus being investigated also seems to play a role with prolonged visual stimuli showing a small mean increase in glutamate of 2.318\% ({\textpm}1.227\%) while at the other extreme, pain stimuli show a mean stimulation effect of 14.458\% ({\textpm}3.736\%). These differences are discussed with regards to possible physiologic interpretations, as well experimental design implications.},
  langid = {english},
  pmid = {29356002},
  keywords = {Brain Chemistry,Brain Mapping,fMRS,functional magnetic resonance spectroscopy,glutamate,Glutamic Acid,Humans,Magnetic Resonance Spectroscopy,Models Neurological,neural activity,neuro-chemistry,neuroimaging,Neurotransmitter Agents},
  file = {/home/weberam2/Zotero/storage/G5C5Z4JN/Mullins_2018_Towards_a_theory_of_functional_magnetic_resonance_spectroscopy_(fMRS).pdf}
}

