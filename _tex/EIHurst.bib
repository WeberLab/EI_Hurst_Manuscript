@article{heScaleFreePropertiesFunctional2011,
  title = {Scale-{{Free Properties}} of the {{Functional Magnetic Resonance Imaging Signal}} during {{Rest}} and {{Task}}},
  author = {He, Biyu J.},
  year = {2011},
